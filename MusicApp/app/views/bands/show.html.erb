<h3><%= @band.name %></h3>

<%= button_to "Back", bands_url, method: :get %>

<ul>
  <% @band.albums.each do |album| %>
    <li><%= link_to album.name, album_url(album) %></li>
  <% end %>
</ul>

<%= button_to "Add Album", new_band_album_url(@band), method: :get %>
<%= button_to "Edit Band", edit_band_url(@band), method: :get %>
<%= button_to "Delete Band", band_url(@band) , method: :delete %>
